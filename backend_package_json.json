{
  "name": "admetrics-backend",
  "version": "1.0.0",
  "description": "AdMetrics AI Dashboard Backend API",
  "main": "dist/app.js",
  "scripts": {
    "dev": "concurrently \"npm run watch\" \"npm run serve\"",
    "watch": "tsc -w",
    "serve": "nodemon dist/app.js",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "docker:build": "docker build -t admetrics-backend .",
    "docker:run": "docker run -p 3000:3000 admetrics-backend",
    "clean": "rimraf dist",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "keywords": [
    "advertising",
    "analytics",
    "ai",
    "dashboard",
    "api",
    "nodejs",
    "typescript",
    "prisma",
    "google-ads",
    "facebook-ads"
  ],
  "author": "AdMetrics Team <dev@admetrics.ai>",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.5.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.3",
    "socket.io": "^4.7.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "zod": "^3.22.4",
    "winston": "^3.10.0",
    "ioredis": "^5.3.2",
    "nodemailer": "^6.9.7",
    "handlebars": "^4.7.8",
    "axios": "^1.5.1",
    "google-ads-api": "^14.0.0",
    "facebook-ads-api": "^0.9.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "csv-parse": "^5.5.2",
    "csv-stringify": "^6.4.4",
    "exceljs": "^4.4.0",
    "puppeteer": "^21.4.1",
    "cron": "^2.4.4",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/cors": "^2.8.15",
    "@types/compression": "^1.7.4",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/bcryptjs": "^2.4.5",
    "@types/node": "^20.8.7",
    "@types/nodemailer": "^6.4.13",
    "@types/multer": "^1.4.9",
    "@types/lodash": "^4.14.200",
    "@types/uuid": "^9.0.6",
    "@types/jest": "^29.5.7",
    "@types/supertest": "^2.0.15",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "tsx": "^3.14.0",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2",
    "prisma": "^5.5.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "eslint": "^8.52.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "prettier": "^3.0.3",
    "lint-staged": "^15.0.2",
    "husky": "^8.0.3",
    "rimraf": "^5.0.5",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.ts",
      "**/src/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/**/*.test.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.ts"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/admetrics-ai-dashboard.git",
    "directory": "backend"
  },
  "bugs": {
    "url": "https://github.com/your-org/admetrics-ai-dashboard/issues"
  },
  "homepage": "https://admetrics.ai"
}