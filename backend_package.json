{
  "name": "admetrics-backend",
  "version": "1.0.0",
  "description": "AdMetrics AI Dashboard - Backend API",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "db:seed": "npx prisma db seed",
    "docker:build": "docker build -t admetrics-backend .",
    "docker:run": "docker run -p 3000:3000 admetrics-backend"
  },
  "keywords": [
    "admetrics",
    "dashboard",
    "ai",
    "advertising",
    "analytics"
  ],
  "author": "AdMetrics Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "express-rate-limit": "^6.10.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-google-oauth20": "^2.0.0",
    "joi": "^17.9.2",
    "zod": "^3.22.2",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.4",
    "csv-parser": "^3.0.0",
    "json2csv": "^6.1.0",
    "node-cron": "^3.0.2",
    "ws": "^8.13.0",
    "socket.io": "^4.7.2",
    "axios": "^1.5.0",
    "googleapis": "^122.0.0",
    "google-ads-api": "^11.6.0",
    "facebook-nodejs-business-sdk": "^18.0.0",
    "linkedin-api-js-client": "^2.4.0",
    "twitter-api-v2": "^1.15.0",
    "@prisma/client": "^5.2.0",
    "ioredis": "^5.3.2",
    "bull": "^4.11.3",
    "nodemailer": "^6.9.4",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "openai": "^4.0.0",
    "anthropic": "^0.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/compression": "^1.7.2",
    "@types/morgan": "^1.9.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/uuid": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/ws": "^8.5.5",
    "@types/node-cron": "^3.0.8",
    "@types/nodemailer": "^6.4.9",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/jest": "^29.5.4",
    "@types/supertest": "^2.0.12",
